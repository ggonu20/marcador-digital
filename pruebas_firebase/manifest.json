{
    "manifest_version" : 3,
    "name": "Marcador Digital",
    "description": "Sigue el precio de un producto",
    "version" : "0.1",
    "host_permissions": ["<all_urls>"],
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "tabs",
        "scripting",
        "activeTab",
        "identity",
        "storage",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon_48.png",
        "default_title": "Una extension de prueba"
    },
    "icons":{
        "16" : "icons/icon_16.png",
        "48" : "icons/icon_48.png",
        "128" : "icons/icon_128.png"
    },
    "options_page": "options.html"
}