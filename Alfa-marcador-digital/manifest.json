{
    "manifest_version" : 3,
    "name": "Marcador Digital",
    "description": "Sigue el precio de un producto",
    "version" : "0.1",
    "host_permissions": ["<all_urls>"],
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "tabs",
        "scripting",
        "activeTab",
        "identity",
        "storage",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png",
        "default_title": "Una extension de prueba"
    },
    "icons":{
        "16" : "icons/icon_16.png",
        "48" : "icon/icon_48.png",
        "128" : "icon/icon_128.png"
    },
    "options_page": "options.html",
    "oauth2": {
        "client_id": "<fill_me>", 
        "scopes": [
          "https://www.googleapis.com/auth/userinfo.email",
          "https://www.googleapis.com/auth/userinfo.profile"
        ]
      },
      "key": "-----BEGIN PUBLIC KEY-----\n<Your Chrome Extension Public Key\n-----END PUBLIC KEY-----"

}