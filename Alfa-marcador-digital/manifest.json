{
    "manifest_version": 3,
    "name": "Marcador Digital",
    "description": "Una aplicacion para seguir el precio de productos",
    "version": "0.0",
    
    "icons": {
      "16": "icons/icon_16.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    },

    "permissions": [
        "activeTab",
        "scripting"
      ],
    "homepage_url": "/html/home.html",
    "action": {
        "default_title": "Click me",
        "default_popup": "/html/popup.html"
    }
    
  }