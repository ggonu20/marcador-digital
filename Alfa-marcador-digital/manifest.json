{
    "manifest_version" : 3,
    "name": "Marcador Digital",
    "description": "Sigue el precio de un producto",
    "version" : "0.1",
    "host_permissions": ["<all_urls>"],
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "tabs",
        "scripting",
        "activeTab",
        "identity",
        "storage",
        "notifications"
    ],
    "action": {
        "default_popup": "./html/popup.html",
        "default_icon": "./icons/icon_48.png",
        "default_title": "Una extension de prueba"
    },
    "icons":{
        "16" : "icons/icon_16.png",
        "48" : "icons/icon_48.png",
        "128" : "icons/icon_128.png"
    },
    "options_page": "./html/options.html",
    "oauth2": {
        "client_id": "686306553948-4v4o8sc8l678nhle6g17eq07dd57a7er.apps.googleusercontent.com", 
        "scopes": [
          "https://www.googleapis.com/auth/userinfo.email",
          "https://www.googleapis.com/auth/userinfo.profile"
        ]
      },
      "key": "-----BEGIN PUBLIC KEY-----\\n-----END PUBLIC KEY-----"

}